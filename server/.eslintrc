{
    "extends": [
        "standard",
        "prettier",
        "prettier/standard"
    ],
    "plugins": [
        "import",
        "prettier",
        "standard"
    ],
    "parserOptions" : {
        "ecmaVersion": 2017
    },
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "prettier/prettier": 2,
        "semi": "error"
    }
}